<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag login_path do %>
<div class="field">
	<%= label_tag :email %>
	<%= text_field_tag :email %>
</div>
	
<div class="field">
	<%= label_tag :password %>
	<%= text_field_tag :password %>
</div>

<%= submit_tag 'Log in' %>

<% end %>